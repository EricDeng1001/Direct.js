import React from 'react';
import makePage from 'renext-core/makePage';

import { view as UserManager } from "../Connected";

class UserPage extends React.Component {
  render(){
    const { history } = this.props;
    return (
      <UserManager
        onCancel={history.goBack}
        onSuccess={history.goBack}
      />
    );
  }
};

export default makePage( UserPage );
