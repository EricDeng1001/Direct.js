import React from 'react';
import makePage from 'HOC/makePage';

import SlideLR from 'Animation/SlideLR';
import SlideRL from 'Animation/SlideRL';
import SlideUD from 'Animation/SlideUD';
import SlideDU from 'Animation/SlideDU';

import SplitToPages from 'Wrapper/SplitToPages';

class UIPage extends React.Component {
  constructor( props ){
    super( props );
    this.state = {
      capcity: 0
    };
  }

  componentDidMount(){
  }

  render(){
    const { openWindow , ined } = this.props;

    return (
      <React.Fragment>
        <SlideUD play={ined}>
          <SplitToPages capcity={this.state.capcity}>
          {
            (() => {
              let res = [];
              for( let i = 1 ; i <= 300 ; i++ ){
                res.push(<span>&nbsp;{i}</span>);
              }
              return res;
            })()
          }
          </SplitToPages>
        </SlideUD>
      </React.Fragment>
    );
  }
};

export default makePage( UIPage );
