import React from 'react';
import Button from 'UI/Button';

class Alert extends React.Component {
  constructor( props ){
    super( props );
  }

  render(){
    const { closeAlert } = this.props;
    return (
      <div>
        <h1>{this.props.text}</h1>
        <Button
          onClick={closeAlert}
          text="чбошод"
        />
      </div>
    );
  }
}

export default Alert;
